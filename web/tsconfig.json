{
  "compilerOptions": {
    "outDir": "./lib",
    "sourceMap": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "target": "es5",
    "moduleResolution": "node",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es6",
      "dom"
    ]
  },
  "exclude": [
    "./src/api.js",
    "./src/model.js",
    "./src/metarpheus/api.js",
    "./src/metarpheus/model.js"
  ],
  "include": [
    "./src/*",
    "typings"
  ]
}
